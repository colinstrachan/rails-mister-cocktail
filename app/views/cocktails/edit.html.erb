<%= render './cocktails/navbar' %>
<div class="container">
  <div class="wrapper-form">
    <h1>Edit cocktail (<%= @cocktail.name %>)</h1>
    <%= form_for(@cocktail) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, :value => @cocktail.name %>
    <br/>
    <%= f.submit %>
    <% end %>
  </div>
</div>
